<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Express photo app. ></p>
    <div id="photos">
    	<% photos.forEach(function(photo) { %>
    		<div class="photo">
    		 <h2><%=photo.name%></h2>
    		 <a href='/photo/<%=photo.id%>/download'>
    		 <img src="/photos/<%=photo.path%>" />
    		 </a>
    		 </div>
    	<% }) %>
    </div>
  </body>
</html>